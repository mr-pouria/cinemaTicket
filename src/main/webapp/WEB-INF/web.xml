    </context-param>
    <context-param>
        <param-name>smsIrUsername</param-name>
        <param-value>09922466803</param-value>
    </context-param>
    <context-param>
        <param-name>smsIrLine</param-name>
        <param-value>30007732909580</param-value>
    </context-param>
    <welcome-file-list>
        <welcome-file>main</welcome-file>
